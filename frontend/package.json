{
  "name": "emeezo-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${EMZ_FRONTEND_APP_PORT:-5000}",
    "debug": "NODE_OPTIONS='--inspect=0.0.0.0' next dev -p ${EMZ_FRONTEND_APP_PORT:-5000}",
    "dev:ci": "NEXT_PUBLIC_EMZ_BACKEND_APP_URI=http://backend:${EMZ_BACKEND_APP_PORT:-5000} next dev -p ${EMZ_FRONTEND_APP_PORT:-5000}",
    "build": "rm -Rf .next && next build && next export -o dist",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start -p ${EMZ_FRONTEND_APP_PORT:-5000}",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "lint-fix": "npm run lint -- --fix --quiet",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "test": "run-p lint test:unit test:e2e",
    "test:unit": "jest -c test/jest.config.js",
    "test:watch": "npm run test:unit -- --watch",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:e2e:dev": "CYPRESS_BASE_URL=http://localhost:${EMZ_FRONTEND_APP_PORT:-5000} cypress open --env BACKEND_URL=http://localhost:${EMZ_BACKEND_APP_PORT:-8082}",
    "test:e2e": "CYPRESS_BASE_URL=http://localhost:${EMZ_FRONTEND_APP_PORT:-5000} cypress run --env BACKEND_URL=http://backend:${EMZ_BACKEND_APP_PORT:-8082}",
    "test:e2e:ci": "CYPRESS_BASE_URL=http://localhost:${EMZ_FRONTEND_APP_PORT:-5000} start-test dev:ci $EMZ_FRONTEND_APP_PORT 'cypress run --parallel --record --key ${CYPRESS_RECORD_KEY} --ci-build-id ${CYPRESS_BUILD_ID} --env BACKEND_URL=http://backend:${EMZ_BACKEND_APP_PORT:-5000}'",
    "test:ci": "run-p build lint",
    "pre-commit": "run-p format lint-fix",
    "pre-push": "run-p lint-fix",
    "apollo:sync": "bash ./bin/apollo-sync.sh",
    "storybook": "start-storybook -p ${STORYBLOCK_PORT:-6006}",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@monaco-editor/react": "^4.2.2",
    "@reduxjs/toolkit": "^1.5.1",
    "@rjsf/core": "^3.0.0",
    "@types/deep-diff": "^1.0.1",
    "apollo-link-scalars": "^2.1.3",
    "axios": "^0.21.1",
    "bson-objectid": "^2.0.1",
    "deep-diff": "^1.0.2",
    "deepmerge": "^4.2.2",
    "fabric": "^4.5.1",
    "fontfaceobserver": "^2.1.0",
    "formik": "^2.2.6",
    "graphql": "^15.4.0",
    "i18next": "^20.4.0",
    "iframe-lightbox": "^0.2.10",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "next": "^10.0.9",
    "nextjs-progressbar": "^0.0.11",
    "oidc-react": "^1.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.3",
    "react-h5-audio-player": "^3.7.1",
    "react-i18next": "^11.11.4",
    "react-infinite-scroll-component": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-syntax-highlighter": "^15.4.4",
    "react-toastify": "^7.0.4",
    "react-use": "^17.2.1",
    "react-vis": "1.11.7",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "remotion": "^2.4.2",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@next/bundle-analyzer": "^10.1.2",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-knobs": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.14",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.0",
    "@types/fabric": "^4.5.0",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-table": "^7.7.2",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "apollo": "^2.32.1",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "6.2.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "next-compose-plugins": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "react-table": "^7.7.0",
    "start-server-and-test": "^1.11.7",
    "storybook-addon-styled-component-theme": "^1.3.0",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "utility-types": "^3.10.0"
  },
  "moduleNameMapper": {
    "@lib/(.*)": "<rootDir>/libs/$1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint-fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run pre-push"
    }
  }
}
